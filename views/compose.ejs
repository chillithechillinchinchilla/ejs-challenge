<%- include("partials/header"); -%>


<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <textarea class="form-control" name="postTitle" rows="1" cols="100"></textarea>
  </div>
  <div class="form-group">
    <label for="postContent">Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="100"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>

</form>



<%- include("partials/footer"); -%>
